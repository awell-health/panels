{
  "name": "@panels/bots",
  "version": "0.1.0",
  "description": "Awell Panels Bots using Medplum SDK",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "typescript": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "run-s clean:outputs build:outputs",
    "build:outputs": "tsc -p ./tsconfig.json && tsc-alias",
    "clean": "rm -rf .turbo node_modules",
    "clean:outputs": "rimraf ./dist",
    "dev": "run-p dev:start typecheck",
    "dev:start": "tsx watch --inspect --env-file=.env src/index.ts",
    "lint": "biome lint ./src",
    "format": "biome format ./src",
    "start": "node --env-file=.env --enable-source-maps dist/index.js",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "test": "NODE_OPTIONS='--import tsx' vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "NODE_OPTIONS='--import tsx' vitest watch"
  },
  "keywords": ["awell", "bots", "medplum", "healthcare"],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.15.29",
    "esbuild": "^0.25.5",
    "fastify-tsconfig": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.8",
    "tsc-files": "^1.1.4",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "@medplum/core": "^3.0.0",
    "@medplum/fhirtypes": "^3.0.0"
  }
} 